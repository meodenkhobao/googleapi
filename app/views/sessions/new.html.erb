<br/><br/><br/><br/><br/>
<div class="jumbotron">
  <div class="row">
    <div class="col-md-6">
      <%= image_tag asset_url("google.png") %>
    </div>
    <div class="col-md-6">
      <h1>Welcome</h1>
      <h2>Please connect to your Google account to continue.</h2>
      <br/>
      <%= link_to "/auth/google_oauth2" do %>
        <button class="btn btn-lg btn-google-plus">
          <i class="fa fa-google-plus"></i> | Signin with Google
        </button>
      <% end %>
    </div>
  </div>
</div>